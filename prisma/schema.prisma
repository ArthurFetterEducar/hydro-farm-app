generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model teste {
  id                       BigInt    @id @default(autoincrement())
  created_at               DateTime? @default(now()) @db.Timestamptz(6)
  Temperature              String?
  Humidity                 String?
  Petier_Hot_Temperature   Json?     @default("3") @db.Json
  Petier_Hot_State         Json?     @default("0") @db.Json
  Peltier_Cold_Temperature Json?     @default("4") @db.Json
  Peltier_Cold_State       Json?     @default("1") @db.Json
  Water_Level              String?
  Pump_State               Json?     @default("1") @db.Json
}
